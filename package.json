{
  "name": "cleverfit",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.21"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:scripts": "eslint ./ ./src/ --ext .js,.jsx,.ts,.tsx",
    "lint": "yarn lint:css && yarn lint:scripts",
    "lint:fix": "yarn lint:css --fix && yarn lint:scripts --fix",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:e2e": "start-server-and-test start http-get://localhost:3000 cy:open-e2e"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.1",
    "@reduxjs/toolkit": "^2.0.1",
    "antd": "^4.24.15",
    "classnames": "^2.5.1",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@types/css-modules": "^1.0.5",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^4.2.1",
    "arui-presets-lint": "^6.1.0",
    "cypress": "^12.7.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "~3.1.5",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-dirnames": "^1.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "husky": "^4.3.8",
    "kebab-case": "1.0.0",
    "lint-staged": "^12.5.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "prettier": "^2.8.1",
    "start-server-and-test": "^2.0.3",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^5.0.6"
  },
  "prettier": "arui-presets-lint/prettier",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write",
      "stylelint"
    ]
  },
  "packageManager": "yarn@1.22.21"
}
